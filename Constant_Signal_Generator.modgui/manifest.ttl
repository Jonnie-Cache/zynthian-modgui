@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/const>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-constant-signal-generator.html> ;
        modgui:stylesheet <modgui/stylesheet-constant-signal-generator.css> ;
        modgui:screenshot <modgui/screenshot-constant-signal-generator.png> ;
        modgui:thumbnail <modgui/thumbnail-constant-signal-generator.png> ;
        modgui:brand "SWH" ;
        modgui:label "Constant Signal Generator" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "red" ;
        modgui:knob "copper" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "amplitude" ;
            lv2:name "Amplitude" ;
        ] ;
    ] .