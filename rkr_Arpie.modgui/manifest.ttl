@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Arpie>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-arpie.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-arpie.css> ;
        modgui:screenshot <modgui/screenshot-rkr-arpie.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-arpie.png> ;
        modgui:brand "Rakarrack " ;
        modgui:label "rkr Arpie" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "brown" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BYPASS" ;
            lv2:name "Bypass" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Wet/Dry" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "PAN" ;
            lv2:name "Panning" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Tempo" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LR_DELAY" ;
            lv2:name "L/R Delay Offset" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "LRCr" ;
            lv2:name "L/R Cross" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "FEEDBACK" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "DAMP" ;
            lv2:name "Damping" ;
        ] ;
    ] .