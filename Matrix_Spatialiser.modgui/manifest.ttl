@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/matrixSpatialiser>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-matrix-spatialiser.html> ;
        modgui:stylesheet <modgui/stylesheet-matrix-spatialiser.css> ;
        modgui:screenshot <modgui/screenshot-matrix-spatialiser.png> ;
        modgui:thumbnail <modgui/thumbnail-matrix-spatialiser.png> ;
        modgui:brand "SWH" ;
        modgui:label "Matrix Spatialis" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "tribal1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "width" ;
            lv2:name "Width" ;
        ] ;
    ] .