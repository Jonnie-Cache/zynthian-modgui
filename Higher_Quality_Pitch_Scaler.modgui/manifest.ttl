@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/pitchScaleHQ>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-higher-quality-pitch-scaler.html> ;
        modgui:stylesheet <modgui/stylesheet-higher-quality-pitch-scaler.css> ;
        modgui:screenshot <modgui/screenshot-higher-quality-pitch-scaler.png> ;
        modgui:thumbnail <modgui/thumbnail-higher-quality-pitch-scaler.png> ;
        modgui:brand "SWH" ;
        modgui:label "HQ Pitch Scaler" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "purple" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "mult" ;
            lv2:name "Pitch" ;
        ] ;
    ] .