@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/combSplitter>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-comb-splitter.html> ;
        modgui:stylesheet <modgui/stylesheet-comb-splitter.css> ;
        modgui:screenshot <modgui/screenshot-comb-splitter.png> ;
        modgui:thumbnail <modgui/thumbnail-comb-splitter.png> ;
        modgui:brand "SWH" ;
        modgui:label "Comb Splitter" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "zinc" ;
        modgui:knob "steel" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Band Separation" ;
        ] ;
    ] .