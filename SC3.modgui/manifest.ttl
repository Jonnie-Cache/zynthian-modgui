@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/sc3>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-sc3.html> ;
        modgui:stylesheet <modgui/stylesheet-sc3.css> ;
        modgui:screenshot <modgui/screenshot-sc3.png> ;
        modgui:thumbnail <modgui/thumbnail-sc3.png> ;
        modgui:brand "SWH" ;
        modgui:label "SC3" ;
        modgui:model "lata" ;
        modgui:panel "7-knobs" ;
        modgui:color "darkblue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "attack" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "release" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "threshold" ;
            lv2:name "Threshold" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "ratio" ;
            lv2:name "Ratio" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "knee" ;
            lv2:name "Knee" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "makeup_gain" ;
            lv2:name "Makeup" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "chain_bal" ;
            lv2:name "Balance" ;
        ] ;
    ] .