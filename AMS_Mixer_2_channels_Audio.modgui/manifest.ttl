@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://avwlv2.sourceforge.net/plugins/avw/mixer_2ch_audio>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-ams-mixer-2-channels-audio.html> ;
        modgui:stylesheet <modgui/stylesheet-ams-mixer-2-channels-audio.css> ;
        modgui:screenshot <modgui/screenshot-ams-mixer-2-channels-audio.png> ;
        modgui:thumbnail <modgui/thumbnail-ams-mixer-2-channels-audio.png> ;
        modgui:brand "Aur√©lien " ;
        modgui:label "AMS Mixer 2 chan" ;
        modgui:model "boxy" ;
        modgui:panel "3-knobs" ;
        modgui:color "zinc" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "gain" ;
            lv2:name "Gain" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "volume1" ;
            lv2:name "Volume 1" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "volume2" ;
            lv2:name "Volume 2" ;
        ] ;
    ] .