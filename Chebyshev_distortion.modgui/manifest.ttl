@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/chebstortion>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-chebyshev-distortion.html> ;
        modgui:stylesheet <modgui/stylesheet-chebyshev-distortion.css> ;
        modgui:screenshot <modgui/screenshot-chebyshev-distortion.png> ;
        modgui:thumbnail <modgui/thumbnail-chebyshev-distortion.png> ;
        modgui:brand "SWH" ;
        modgui:label "ChebyShev Distortion" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "zinc" ;
        modgui:knob "steel" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "dist" ;
            lv2:name "Distortion" ;
        ] ;
    ] .