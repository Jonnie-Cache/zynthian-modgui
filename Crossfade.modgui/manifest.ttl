@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/xfade>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-crossfade.html> ;
        modgui:stylesheet <modgui/stylesheet-crossfade.css> ;
        modgui:screenshot <modgui/screenshot-crossfade.png> ;
        modgui:thumbnail <modgui/thumbnail-crossfade.png> ;
        modgui:brand "SWH" ;
        modgui:label "Crossfade" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "petrol" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "xfade" ;
            lv2:name "Crossfade" ;
        ] ;
    ] .