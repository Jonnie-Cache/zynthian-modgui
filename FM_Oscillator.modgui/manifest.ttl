@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/fmOsc>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-fm-oscillator.html> ;
        modgui:stylesheet <modgui/stylesheet-fm-oscillator.css> ;
        modgui:screenshot <modgui/screenshot-fm-oscillator.png> ;
        modgui:thumbnail <modgui/thumbnail-fm-oscillator.png> ;
        modgui:brand "SWH" ;
        modgui:label "FM Oscillator" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "zinc" ;
        modgui:knob "steel" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "wave" ;
            lv2:name "Waveform" ;
        ] ;
    ] .