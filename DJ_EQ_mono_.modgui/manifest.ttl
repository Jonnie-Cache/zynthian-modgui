@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/dj_eq_mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-dj-eq-mono.html> ;
        modgui:stylesheet <modgui/stylesheet-dj-eq-mono.css> ;
        modgui:screenshot <modgui/screenshot-dj-eq-mono.png> ;
        modgui:thumbnail <modgui/thumbnail-dj-eq-mono.png> ;
        modgui:brand "SWH" ;
        modgui:label "DJ EQ" ;
        modgui:model "boxy" ;
        modgui:panel "3-knobs" ;
        modgui:color "slime" ;
        modgui:knob "green" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "lo" ;
            lv2:name "Lo gain" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "mid" ;
            lv2:name "Mid gain" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "hi" ;
            lv2:name "Hi gain" ;
        ] ;
    ] .