@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#reve>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-reverb.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-reverb.css> ;
        modgui:screenshot <modgui/screenshot-rkr-reverb.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-reverb.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Reverb" ;
        modgui:model "japanese" ;
        modgui:panel "7-a-knobs" ;
        modgui:color "gray" ;
        modgui:knob "gray" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BYPASS" ;
            lv2:name "Bypass" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Wet/Dry" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "PAN" ;
            lv2:name "Panning" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "TIME" ;
            lv2:name "Time" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "IDEL" ;
            lv2:name "Initial Delay" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "IDELFB" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "LPF" ;
            lv2:name "Lowpass" ;
        ] ;
    ] .