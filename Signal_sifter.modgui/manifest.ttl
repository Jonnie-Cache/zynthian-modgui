@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/sifter>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-signal-sifter.html> ;
        modgui:stylesheet <modgui/stylesheet-signal-sifter.css> ;
        modgui:screenshot <modgui/screenshot-signal-sifter.png> ;
        modgui:thumbnail <modgui/thumbnail-signal-sifter.png> ;
        modgui:brand "SWH" ;
        modgui:label "Signal Sifter" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "petrol" ;
        modgui:knob "petrol" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "size" ;
            lv2:name "Sift size" ;
        ] ;
    ] .